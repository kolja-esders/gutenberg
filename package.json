{
  "name": "gutenberg",
  "version": "1.0.0",
  "scripts": {
    "start": "yarn run clean && webpack-dev-server",
    "build": "yarn run clean && NODE_ENV=production webpack",
    "lint": "eslint client && prospector src--uses django --ignore-patterns */migrations",
    "lint-fix": "eslint --fix client && autopep8 --in-place --aggressive --aggressive --recursive --max-line-length 79 server",
    "clean": "rm -rf ./static/bundles/* ./static/index.html",
    "postinstall": "python3 ./manage.py graphql_schema && npm run build",
    "relay": "relay-compiler --watch --src ./client --schema ./lib/schema.json",
    "flow-server": "flow server",
    "flow": "flow status"
  },
  "engines": {
    "node": ">=8.1.2",
    "npm": ">=5.0.3"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-flow": "^6.23.0",
    "chalk": "^1.1.3",
    "empty": "^0.10.1",
    "eslint": "^3.17.0",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "^6.8.0",
    "flow-bin": "^0.47.0",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "^1.3.3",
    "webpack-dev-server": "2.4.2"
  },
  "dependencies": {
    "autoprefixer": "^6.5.3",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.8",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-relay": "^1.0.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "classnames": "^2.2.5",
    "connect-history-api-fallback": "^1.3.0",
    "cross-env": "^3.1.3",
    "css-loader": "^0.26.1",
    "deep-freeze": "^0.0.1",
    "dotenv": "^4.0.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "farce": "^0.2.1",
    "file-loader": "^0.11.2",
    "found": "^0.3.4",
    "found-relay": "^0.3.0-alpha.9",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "lodash": "^4.17.2",
    "moment": "^2.21.0",
    "node-sass": "^4.5.3",
    "normalize.css": "^5.0.0",
    "postcss-loader": "^1.2.0",
    "precss": "^1.4.0",
    "react": "^15.4.1",
    "react-body-classname": "^1.2.0",
    "react-dom": "^15.4.1",
    "react-dropdown": "^1.2.0",
    "react-dropzone": "^4.2.3",
    "react-helmet": "^5.2.0",
    "react-hot-loader": "3.0.0-beta.6",
    "react-mdc-web": "^0.12.2",
    "react-moment": "^0.7.0",
    "react-relay": "^1.4.0",
    "react-relay-network-layer": "^2.0.1",
    "recharts": "^1.0.0-alpha.2",
    "relay-compiler": "^1.0.0",
    "relay-runtime": "^1.0.0",
    "sass-loader": "^6.0.5",
    "semantic-ui-less": "^2.2.12",
    "semantic-ui-react": "^0.72.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "url-search-params": "^0.10.0",
    "webpack": "^3.4.1",
    "webpack-bundle-tracker": "^0.2.0"
  },
  "metadata": {
    "graphql": {
      "schema": "./build/schema.json"
    }
  },
  "babel": {
    "presets": [
      "react",
      [
        "env",
        {
          "targets": {
            "browsers": [
              "last 1 versions"
            ],
            "node": "current"
          }
        }
      ],
      "stage-0"
    ],
    "plugins": [
      "relay",
      "transform-decorators-legacy"
    ],
    "env": {
      "development": {
        "plugins": [
          "react-hot-loader/babel"
        ]
      }
    }
  }
}
